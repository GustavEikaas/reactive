{
	"name": "@gustav-e/reactive",
	"private": false,
	"description": "React library for complex state managment",
	"version": "0.0.1",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"jsdelivr": "dist/bundle.min.js",
	"unpkg": "dist/bundle.min.js",
	"scripts": {
		"prebuild": "npm run clean",
		"build": "tsc && tsc-alias",
		"postbuild": "npm run rollup && npm run prepare-packages && npm run prepare-dist",
		"rollup": "rollup -c tools/rollup.config.js",
		"prepare-packages": "node tools/prepare-package.js",
		"prepare-dist": "node tools/prepare-dist.js",
		"prepare": "npm run build",
		"clean": "rimraf -r dist"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"eslint": "^8.23.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.7.1",
		"typescript": "^4.6.4",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"rimraf": "^3.0.2",
		"rollup": "^2.56.3",
		"rollup-plugin-terser": "^7.0.2",
		"tsc-alias": "^1.3.9"
	},
	"license": "MIT",
	"author": "https://www.github.com/GustavEikaas",
	"repository": "https://github.com/GustavEikaas/reactive.git",
	"keywords": [
		"typescript",
		"react",
		"state"
	],
	"publishConfig": {
		"access": "public"
	}
}
